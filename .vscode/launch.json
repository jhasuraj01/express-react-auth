{
    "type": "pwa-node",
    "request": "launch",
    "name": "Debug Dev Env",
    "runtimeArgs": [
        "-r",
        "ts-node/register",
        "-r",
        "tsconfig-paths/register",
    ],
    "args": [
        "${workspaceFolder:express-gen-ts}/server/src/index.ts"
    ],
    "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**"
    ],
}